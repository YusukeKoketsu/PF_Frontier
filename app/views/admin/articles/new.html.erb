<h1>新規投稿</h1>

<%= form_with model: @article, url: admin_articles_path, method: :post do |f| %>

<%= f.label :image, "画像" %>
<%= f.file_field :image, accept: "image/*" %>

<%= f.label :title, "タイトル" %>
<%= f.text_field :title %>

<%= f.label :introduction, "紹介文" %>
<%= f.text_area :introduction %>

<%= f.submit "投稿" %>

<% end %>
